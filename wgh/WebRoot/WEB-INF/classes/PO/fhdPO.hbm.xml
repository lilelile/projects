<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="PO.fhdPO" table="fhd" catalog="yunhuo">
		<id name="id" type="java.lang.String" >
			<column name="id"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="film" type="java.lang.String" column="film"></property>
		<property name="type" type="java.lang.String" column="type"></property>
		<property name="date" type="java.lang.String" column="date"></property>
		<property name="seat" type="java.lang.String" column="seat"></property>
		<property name="cost" type="java.lang.String" column="cost"></property>
	</class>





</hibernate-mapping>